{
    "name": "ocr-extraction-worker",
    "version": "1.0.0",
    "description": "Cloud Run worker for Document AI OCR processing",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "docker:build": "docker build -t ocr-extraction-worker .",
        "docker:run": "docker run -p 8080:8080 --env-file .env ocr-extraction-worker"
    },
    "dependencies": {
        "@google-cloud/documentai": "^8.0.0",
        "@supabase/supabase-js": "^2.38.0",
        "dotenv": "^16.3.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0",
        "ts-node": "^10.9.0"
    },
    "engines": {
        "node": ">=18"
    }
}